<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qy.mapper.ClassKindMapper">
    <insert id="add">
        insert into classkind values(null,#{name},#{cost},#{duration});
    </insert>
    <update id="update">
        update classkind set name=#{name},cost=#{cost},duration=#{duration} where id=#{id}
    </update>
    <delete id="delete">
        delete from classKind where id=#{id}
    </delete>
    <select id="findAll" resultType="com.qy.domain.ClassKind">
        select * from classkind
    </select>
</mapper>